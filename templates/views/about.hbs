    <!-- About Page Content -->

    <div class="container-fluid header">
      <div class="row">
        <div class="col-md-10 col-12 mx-auto">
          <div class="row">
            <div class="col-md-6 col-12 about-left">
              <figure>
                <img
                  src="https://raw.githubusercontent.com/thapatechnical/webproject/master/public/images/about.png"
